local Button = MainTab:CreateButton({
    Name = "üîÑRejoin gameüîÑ",
    Interact = 'Click',
    Callback = function()
        warn"[LOCAL SERVER] !rejoin command send to server"
local TeleportService = game:GetService("TeleportService")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local Rejoin = coroutine.create(function()
    local Success, ErrorMessage = pcall(function()
        TeleportService:Teleport(game.PlaceId, LocalPlayer)
    end)

    if ErrorMessage and not Success then
        warn(ErrorMessage)
    end
end)
wait()
print"Rejoin Game"
wait()
coroutine.resume(Rejoin)
print("Rejoinning..")
--Thanks for using this script
    end,
})

local Section = MainTab:CreateSection("Executors [Mobile]", 112958886)

local Button = MainTab:CreateButton({
    Name = "CodeX", 
    Interact = 'Click',
    Callback = function()
        setclipboard("https://www.codex.lol")
        ArrayField:Notify({
   Title = "Copied to Keyboard‚òëÔ∏è",
   Content = "Paste The Link on Brownser",
   Duration = 6.5,
   Image = 11396131982,
   Actions = { -- Notification Buttons
      Ignore = {
         Name = "Okay!",
         Callback = function()
         print("Easter Egg")
      end
   },
 },
})
    end,
})

